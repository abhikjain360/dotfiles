#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'lit==0.10.1.post1','console_scripts','lit'
__requires__ = 'lit==0.10.1.post1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lit==0.10.1.post1', 'console_scripts', 'lit')()
    )
